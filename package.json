{
  "name": "my-turbo-project",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "start-web-app": "cd apps/web && npm run start"
  },
  "devDependencies": {
    "prettier": "^3.6.0",
    "turbo": "^2.5.4",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.4.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "jotai": "^2.12.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
